{
  "name": "mna-trajectoires-pro-server",
  "version": "0.0.1",
  "description": "[MNA] Trajectoires Pro",
  "repository": "https://github.com/mission-apprentissage/trajectoires-pro.git",
  "private": false,
  "author": "MNA",
  "license": "MIT",
  "module": "src/index.js",
  "type": "module",
  "scripts": {
    "ts:run": "node --loader ts-node/esm",
    "ts:generate": "node src/types/index.js",
    "start": "nodemon --ext 'ts,js,cjs,json' --ignore 'tests/' --exec 'yarn ts:run src/index.js $TS_NODE_OPTIONS'",
    "debug": "yarn start --inspect=0.0.0.0 --signal SIGINT",
    "cli": "TRAJECTOIRES_PRO_LOG_DESTINATIONS=stderr yarn ts:run src/cli.js",
    "test": "yarn ts:generate && TRAJECTOIRES_PRO_LOG_LEVEL=fatal mocha --inline-diffs --exit tests/**/*-test.js",
    "coverage": "nyc --temp-dir .coverage/.nyc_output --report-dir .coverage --reporter=lcov --reporter=html yarn test",
    "lint": "eslint src/ tests/"
  },
  "dependencies": {
    "axios": "0.27.2",
    "body-parser": "1.20.0",
    "boom": "7.3.0",
    "bunyan": "1.8.15",
    "bunyan-slack": "0.0.10",
    "commander": "9.2.0",
    "compose-middleware": "5.0.1",
    "csv-parse": "5.0.4",
    "dotenv": "16.0.0",
    "ejs": "3.1.7",
    "env-var": "7.1.1",
    "express": "4.18.1",
    "express-mongo-sanitize": "2.2.0",
    "fast-luhn": "2.0.0",
    "iconv-lite": "0.6.3",
    "joi": "17.6.0",
    "json-schema-to-typescript": "10.1.5",
    "lodash-es": "4.17.21",
    "luxon": "2.3.2",
    "mongodb": "4.5.0",
    "oleoduc": "0.7.0",
    "passport": "0.5.2",
    "passport-headerapikey": "1.2.2",
    "pretty-ms": "7.0.1",
    "rate-limiter-flexible": "2.3.7",
    "stream-json": "1.7.4",
    "swagger-ui-express": "4.3.0",
    "uuid": "8.3.2",
    "yamljs": "0.3.0"
  },
  "engines": {
    "node": ">=12.11.0"
  },
  "prettier": {
    "printWidth": 120,
    "bracketSpacing": true,
    "trailingComma": "es5"
  },
  "devDependencies": {
    "@faker-js/faker": "6.3.1",
    "@types/node": "17.0.41",
    "@typescript-eslint/parser": "5.27.1",
    "axiosist": "0.10.0",
    "chalk": "4.1.2",
    "eslint": "7.1.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-import-resolver-typescript": "2.7.1",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-mocha": "10.0.4",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "3.1.3",
    "lint-staged": ">=10",
    "mocha": "10.0.0",
    "mongodb-memory-server": "8.5.2",
    "nock": "13.2.4",
    "nodemon": "2.0.16",
    "nyc": "15.1.0",
    "prettier": "2.6.2",
    "ts-node": "10.8.1",
    "typescript": "4.7.4"
  },
  "lint-staged": {
    "*.js": [
      "eslint --cache --fix",
      "prettier --write"
    ]
  }
}
