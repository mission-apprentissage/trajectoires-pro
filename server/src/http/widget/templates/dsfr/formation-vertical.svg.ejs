<%- await include('variables'); %>
<%_
deltaHeight=75
height=200 + nbIndicateur * deltaHeight
currentHeight = 10
-%>
<svg version="1.1" width="320" height="<%= height %>" viewBox="0 0 320 <%= height %>" fill="none" xmlns="http://www.w3.org/2000/svg">
  <%- await include('../common/description'); %>
  <%- await include('common'); %>

  <g class="blockTitle">

    <g class="content">
      <rect x="0" y="62" class="border" width="319" height="<%= height - 64 %>" />
      <svg y="110" x="20">
        <g class="blocks">
          <svg y="30">
            <g class="block">
              <% if (stats?.taux_en_emploi_6_mois) { %>
              <svg x="0" y="<%= currentHeight %>" class="block">
                <rect width="60" height="35" />

                <text class="number">
                  <tspan x="30" y="26"><%= stats?.taux_en_emploi_6_mois?.valeur %>%</tspan>
                </text>
                <text class="description">
                  <tspan x="75" y="14"><%= description_en_emploi_6_mois_part[0] %></tspan>
                  <tspan x="75" y="32"><%= description_en_emploi_6_mois_part[1] %></tspan>
                </text>
                <text class="details">
                  <tspan x="75" y="46"><%= details_en_emploi_6_mois[0] %> <%= details_en_emploi_6_mois[1] %></tspan>
                </text>
              </svg>
              <% currentHeight = currentHeight + deltaHeight %>
              <% } %>

              <% if (stats?.taux_en_formation) { %>
              <svg x="0" y="<%= currentHeight %>" class="block">
                <rect width="60" height="35" />

                <text class="number">
                  <tspan x="30" y="26"><%= stats?.taux_en_formation?.valeur %>%</tspan>
                </text>
                <text class="description">
                  <tspan x="75" y="14"><%= description_en_formation %></tspan>
                </text>
                <text class="details">
                  <tspan x="75" y="30"><%= details_en_formation[0] %></tspan>
                  <tspan x="75" y="44"><%= details_en_formation[1] %></tspan>
                </text>
              </svg>
              <% currentHeight = currentHeight + deltaHeight %>
              <% } %>

              <% if (stats?.taux_autres_6_mois) { %>
              <svg x="0" y="<%= currentHeight %>" class="block">
                <rect width="60" height="35" />
                <text class="number">
                  <tspan x="30" y="26"><%= stats?.taux_autres_6_mois?.valeur %>%</tspan>
                </text>
                <text class="description">
                  <tspan x="75" y="14"><%= description_autres_6_mois %></tspan>
                </text>
                <text class="details">
                  <tspan x="75" y="30"><%= details_autres_6_mois[0] %></tspan>
                  <tspan x="75" y="44"><%= details_autres_6_mois[1] %></tspan>
                </text>
              </svg>
              <% currentHeight = currentHeight + deltaHeight %>
              <% } %>
            </g>

            <text class="about">
              <tspan x="0" y="<%= currentHeight + 14 %>">
                <%= about_short[0] %>
              </tspan>
              <tspan x="0" y="<%= currentHeight + 30 %>">
                <%= about_short[1] %>
              </tspan>
            </text>
          </svg>
        </g>
      </svg>
    </g>

    <g class=" mainTitle">
      <svg>
        <rect width="700" height="120" />
        <text>
          <tspan x="30" y="30"><%= title[0] %></tspan>
          <tspan x="30" y="54"><%= title[1] %></tspan>
          <tspan x="30" y="78"><%= title[2] %></tspan>
          <tspan x="30" y="104" class="region"><%= detailVueEtablissement %></tspan>
        </text>
      </svg>
    </g>
  </g>
</svg>