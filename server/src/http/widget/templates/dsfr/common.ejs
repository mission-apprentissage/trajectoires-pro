<defs>
  <style type="text/css">
    @font-face {
      font-family: "Marianne";
      src: url("data:font/truetype;charset=utf-8;base64,<%= base64Font %>") format("woff");
      font-weight: 500;
      font-style: normal;
      font-display: swap;
    }

    text {
      font-family: Marianne;
      font-style: normal;
    }

    .region text {
      fill: #383838;
      font-weight: 700;
      font-size: 16px;
      line-height: 22px;
    }

    .filiere text {
      fill: #000091;
      font-weight: 700;
      font-size: 20px;
      line-height: 27px;
    }

    .block .title text {
      fill: #000091;
      font-weight: 700;
      font-size: 24px;
      line-height: 32px;
    }

    .block rect {
      fill: #E8EDFF;
    }

    .block .number {
      fill: #0063CB;
      font-weight: 700;
      font-size: 25px;
      line-height: 24px;
      text-anchor: middle;
    }

    .block .description {
      fill: #3A3A3A;
      font-weight: 400;
      font-size: 14px;
      line-height: 18px;
    }

    .block .details {
      fill: #3A3A3A;
      font-weight: 400;
      font-size: 12px;
      font-style: italic;
      line-height: 16px;
    }

    .about {
      fill: #666666;
      font-weight: 400;
      font-size: 12px;
      line-height: 16px;
      font-style: italic;
    }


    .blockTitle>.mainTitle rect {
      fill: #F5F5FE;
    }

    .blockTitle>.mainTitle text {
      fill: #3A3A3A;
      font-weight: 700;
      font-size: 20px;
      line-height: 24px;
    }

    .blockTitle .region {
      fill: #383838;
      font-weight: 700;
      font-size: 14px;
      line-height: 19px;
    }

    .content .border {
      stroke: #E5E5E5;
    }

    .borderTopFiliere {
      fill: #000091;
    }

    .borderFiliere {
      stroke: #E5E5E5;
    }
  </style>
</defs>