<defs>
  <style type="text/css">
    @font-face {
      font-family: "Marianne";
      src: url("data:font/truetype;charset=utf-8;base64,<%= base64Font %>") format("woff");
      font-weight: 500;
      font-style: normal;
      font-display: swap;
    }

    text {
      font-family: Marianne;
      font-style: normal;
    }

    .region text {
      fill: #383838;
      font-weight: 700;
      font-size: 16px;
      line-height: 22px;
    }

    .filiere text {
      fill: #383838;
      font-weight: 700;
      font-size: 16px;
      line-height: 22px;
    }

    .title text {
      fill: #000091;
      font-weight: 700;
      font-size: 24px;
      line-height: 32px;
    }

    .block rect {
      fill: #f6f6f6;
    }

    .block .number {
      fill: #161616;
      font-weight: 700;
      font-size: 40px;
      line-height: 48px;
      text-anchor: middle;
    }

    .block .description {
      fill: #161616;
      font-weight: 400;
      font-size: 16px;
      line-height: 24px;
      text-anchor: middle;
    }

    .block .details {
      fill: #666666;
      font-weight: 400;
      font-size: 12px;
      line-height: 20px;
      text-anchor: middle;
    }

    .about {
      fill: #666666;
      font-weight: 400;
      font-size: 12px;
      line-height: 20px;
    }
  </style>
</defs>