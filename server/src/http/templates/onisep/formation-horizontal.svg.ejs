<svg
  width="700"
  height="209"
  viewBox="0 0 700 209"
  fill="none"
  xmlns="http://www.w3.org/2000/svg"
  xmlns:xlink="http://www.w3.org/1999/xlink"
>
  <% if(meta.title) { %>
  <title><%= meta.title %></title>
  <% } %> <% if(meta.description) { %>
  <desc><%= meta.description%></desc>
  <% } %>
  <rect x="2" y="2" width="696" height="205" fill="white" stroke="#FFD400" stroke-width="4" />

  <text
    fill="black"
    xml:space="preserve"
    style="white-space: pre"
    font-family="Expressway Free"
    font-size="28"
    font-weight="bold"
    letter-spacing="0em"
  >
    <tspan x="30" y="52.625">Que deviennent les apprenants apr&#xe8;s cette formation ?</tspan>
  </text>

  <!-- first block starts here -->
  <g>
    <rect x="30" y="97" width="90" height="35" rx="4" fill="#0D7B92" />
    <text
      fill="white"
      xml:space="preserve"
      style="white-space: pre"
      font-family="Expressway Free"
      font-size="25"
      font-weight="bold"
      letter-spacing="0em"
    >
      <tspan x="49" y="125.438"><%= rates[0].rate %>%</tspan>
    </text>
    <text
      fill="black"
      xml:space="preserve"
      style="white-space: pre"
      font-family="Expressway Free"
      font-size="14"
      letter-spacing="0em"
    >
      <% if(rates[0].labels.length===1) { %>
      <tspan x="129" y="120.334"><%= rates[0].labels[0] %></tspan>
      <% } else { %>
      <tspan x="129" y="110.334"><%= rates[0].labels[0] %></tspan>
      <tspan x="129" y="130.334"><%= rates[0].labels[1] %></tspan>
      <% } %>
    </text>
  </g>
  <!-- end first block -->

  <!-- second block starts here -->
  <% if(rates.length > 1) { %>
  <g>
    <rect x="365" y="97" width="90" height="35" rx="4" fill="#0D7B92" />
    <text
      fill="white"
      xml:space="preserve"
      style="white-space: pre"
      font-family="Expressway Free"
      font-size="25"
      font-weight="bold"
      letter-spacing="0em"
    >
      <tspan x="394" y="125.438"><%= rates[1].rate %>%</tspan>
    </text>
    <text
      fill="black"
      xml:space="preserve"
      style="white-space: pre"
      font-family="Expressway Free"
      font-size="14"
      letter-spacing="0em"
    >
      <% if(rates[1].labels.length === 1) { %>
      <tspan x="465" y="120.334"><%= rates[1].labels[0] %></tspan>
      <% } else { %>
      <tspan x="465" y="110.334"><%= rates[1].labels[0] %></tspan>
      <tspan x="465" y="130.334"><%= rates[1].labels[1] %></tspan>
      <% } %>
    </text>
  </g>
  <% } %>
  <!-- end second block -->

  <text
    fill="#3A3A3A"
    xml:space="preserve"
    style="white-space: pre"
    font-family="Expressway Free"
    font-size="11"
    font-style="italic"
    letter-spacing="0em"
  >
    <tspan x="30" y="176.312">Chiffres issus du site InserJeunes</tspan>
  </text>

  <defs>
    <style>
      @import url("https://fonts.cdnfonts.com/css/expressway-free");
    </style>
  </defs>
</svg>
